<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:jc="clr-namespace:JungleControls">
    <Style TargetType="jc:LazyControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="jc:LazyControl">
                    <ContentPresenter x:Name="Presenter" Content="{x:Null}" ContentTemplate="{x:Null}" ContentTemplateSelector="{x:Null}" ContentStringFormat="{x:Null}" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>